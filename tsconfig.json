{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"outDir": "./dist",
		"rootDir": ".",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"types": ["vite/client", "node", "@types/google.maps"]
	}
}
